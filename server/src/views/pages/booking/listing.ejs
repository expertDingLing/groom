<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../../layouts/global_style.ejs')  %>
        <%- include('../../layouts/theme_style.ejs')  %>
        <!-- PAGE LEVEL PLUGINS -->
        <link href="/assets/global/plugins/bootstrap-tagsinput/bootstrap-tagsinput.css" rel="stylesheet" type="text/css" />
        <link href="/assets/global/plugins/typeahead/typeahead.css" rel="stylesheet" type="text/css" />
        <!-- PAGE LEVEL STYLES -->
        <link href="/assets/pages/css/booking/listing.css" rel="stylesheet" type="text/css" />
    </head>

    <body class="page-header-fixed page-sidebar-closed-hide-logo page-container-bg-solid">

        <%- include('../../layouts/navbar.ejs')  %>

        <!-- BEGIN CONTAINER -->
        <div class="page-container">

            <%- include('../../layouts/sidebar.ejs')  %>

            <!-- BEGIN CONTENT -->
            <div class="page-content-wrapper">
                <!-- BEGIN CONTENT BODY -->
                <div class="page-content">
                    <!-- BEGIN PAGE HEADER-->
                    <div class="page-header">
                        <div class="page-title">
                            <h2> <%= title %>  </h2>
                            <small> <%= path %> </small>
                        </div>
                        <form class="search" action="javascript:;" method="post">
                            <div class="input-group">
                                <input type="text" class="form-control" placeholder="Booking ID">
                                <span class="input-group-btn">
                                    <a href="javascript:;" class="btn submit">
                                        <i class="icon-magnifier"></i>
                                    </a>
                                </span>
                            </div>
                        </form>
                        <button type="button" class="btn btn-help">Help</button>
                    </div>
                    <!-- END PAGE HEADER-->
                    <div class="action-bar">
                        <button type="button" class="btn btn-filter">Filters</button>
                        <button type="button" class="btn btn-action">Actions</button>
                        <div class="btn-toolbar">
                            
                        </div>
                        <div class="tagsinput">
                            <input type="text" value="All Steps" data-role="tagsinput" style="display: none;">
                        </div>
                        <button type="button" class="btn btn-refresh">
                            <i class="fa fa-refresh"></i>
                        </button>
                    </div>
                    
                    <div class="main">
                        
                    </div>
                </div>
                <!-- END CONTENT BODY -->
            </div>
            <!-- END CONTENT -->
        </div>
        <!-- END CONTAINER -->

        <%- include('../../layouts/global_script.ejs')  %>
        <%- include('../../layouts/theme_script.ejs')  %>
        <!-- PAGE LEVEL PLUGINS -->
        <script src="/assets/global/plugins/bootstrap-tagsinput/bootstrap-tagsinput.min.js" type="text/javascript"></script>
        <script src="/assets/global/plugins/typeahead/handlebars.min.js" type="text/javascript"></script>
        <script src="/assets/global/plugins/typeahead/typeahead.bundle.min.js" type="text/javascript"></script>
        <!-- PAGE LEVEL SCRIPTS -->
        <script src="/assets/layout/scripts/components-bootstrap-tagsinput.min.js" type="text/javascript"></script>
        <script src="/assets/pages/scripts/booking/listing.js" type="text/javascript"></script>

    </body>

</html>
